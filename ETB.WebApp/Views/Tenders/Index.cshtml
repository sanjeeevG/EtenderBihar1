<div class="container-fluid" style="padding-left:0px;">
    <div>
        <div id="carouselIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselIndicators" data-slide-to="1"></li>
                <li data-target="#carouselIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/Assests/slide111.jpg" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Assests/slide12.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Assests/slide23.jpg" alt="Third slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-md-10 col-sm-10" style="border:0px solid gray;background-color:none;padding-right:0px;">
            @await Html.PartialAsync("TenderViewPartial")
        </div>
        <div class="col-md-2 col-sm-12" style="border:0px solid gray">
            @await Html.PartialAsync("DistrictWiseTenderCount")
        </div>
    </div>
    <div class="row mt-2 bg-light">
        @await Html.PartialAsync("_cardData") 
    </div>
</div>